<h1> My Delivery Addresses</h1>
<ul>
	<% @addresses.each do |address| %>
		<li>
			<%= address.street %>
			<%= address.apt_num %>
			<%= address.city %>
			<%= address.state%>
			<%= address.zip %>
			<%=link_to("Show Address", address_path(address.id)) %>
		</li>
	<%end%>
</ul>

<%=link_to("Add New Address", new_address_path) %>